#include "main.h"
/**
 * _strcat - function that prints a string in revers
 * @dest: pointer value
 * @src: pointer value
 * Return: the concatenated strings
 */
char *_strcat(char *dest, char *src)
{
	int c = 0;
	while (s[c])
	{
		c = c + 1;
	}
	c--;
	for (; c >= 0; c--)
	{
		_putchar(s[c]);
	}
	_putchar('\n');
}
